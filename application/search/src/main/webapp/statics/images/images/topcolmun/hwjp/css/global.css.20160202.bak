.floor1{
    position: relative;
    background-color: white;
    width: 1225px;
    height: 227px;
    margin: 0 auto;
}
.floor1  ul{
    padding-left: 0px;

}
.floor1  ul li{

    list-style: none;
    float: left;
    width:297px;
    height: 227px;
    margin-left: 8px;
    border: 1px solid white;
}
.floor-wrap {
    width: 1220px;
    height: 702px;
    background-color: white;
    position: relative;
    margin: 0 auto;
    margin-top: 50px;
    display: block;
}
.floor-wrap:last-child{
    padding-bottom: 50px;
}
.floor-wrap a{
    height: auto;
    float: left;
    border: 1px solid lightgray;
}
.floor-wrap .sm{
    height: 232px;
    width: 188px;
    border-left: 0px;
}

.floor-wrap .mi{
    height: 232px;
    width: 377px;
    border-left: 0px;
}
.floor-wrap .hi{
    height: 465px;
    width: 188px;
    border-left: 0px;
}
.floor-wrap .bi{
    height: 698px;
    width: 266px;
}
.floor-wrap .no-top{
    border-top: 0px;
}

.floor1  li a:hover img{
    -webkit-transform:scale(1.1,1.1);
    -moz-transform:scale(1.1,1.1);
    -transform:scale(1.1,1.1);
    -ms-transform:scale(1.1,1.1);


    -webkit-transition-duration:2s;
    -moz-transition-duration:2s;
    -ms-transition-duration:2s;
    transition-duration: 2s;
    -moz-box-shadow: 3px 3px 4px lightgray;
    -webkit-box-shadow: 3px 3px 4px lightgray;
    box-shadow: 3px 3px 4px lightgray;
    /* For IE 8 */
    -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000')";
    /* For IE 5.5 - 7 */
    filter: progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000');
}
.floor-wrap  a:hover .big{
    -webkit-transform:scale(1.1,1.1);
    -moz-transform:scale(1.1,1.1);
    -transform:scale(1.1,1.1);
    -ms-transform:scale(1.1,1.1);

    -webkit-transition-duration:2s;
    -moz-transition-duration:2s;
    -ms-transition-duration:2s;
    transition-duration: 2s;

    -moz-box-shadow: 3px 3px 4px lightgray;
    -webkit-box-shadow: 3px 3px 4px lightgray;
    box-shadow: 3px 3px 4px lightgray;
    /* For IE 8 */
    -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000')";
    /* For IE 5.5 - 7 */
    filter: progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000');
}